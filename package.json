{
  "name": "sc2-api-service",
  "version": "1.0.0",
  "private": true,
  "description": "StarCraft II Community API service",
  "main": "dist/index.js",
  "scripts": {
    "clean": "rimraf dist/*",
    "build": "npm run lint && npm run test && npm run clean && tsc",
    "format": "prettier --write \"src/**/*.ts\" \"src/**/*.js\"",
    "lint": "tslint --project tsconfig.json",
    "runbas": "npm --prefix ../bnet-auth-service start",
    "watch": "tsc-watch --onSuccess \"npm start\"",
    "dev": "parallel runbas watch | pino-pretty",
    "test": "jest",
    "clearTestCache": "jest --clearCache",
    "start": "node scripts/start.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/lukemnet/sc2-api-service.git"
  },
  "keywords": [
    "starcraft",
    "starcraft2",
    "community",
    "api",
    "player",
    "backend"
  ],
  "author": "Łukasz Wójcik",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/lukemnet/sc2-api-service/issues"
  },
  "homepage": "https://github.com/lukemnet/sc2-api-service#readme",
  "dependencies": {
    "blizzapi": "1.1.7",
    "fastify": "3.2.0",
    "fastify-blipp": "3.0.0",
    "fastify-caching": "6.0.1",
    "fastify-env": "2.0.1",
    "fastify-ioredis": "0.1.1",
    "fastify-log": "1.2.1",
    "fastify-plugin": "2.3.0",
    "fastify-redis": "4.0.3",
    "starcraft2-api": "1.3.8"
  },
  "devDependencies": {
    "@types/jest": "26.0.9",
    "@types/node": "14.0.27",
    "fastify-redis-mock": "1.0.0",
    "jest": "26.4.0",
    "npm-parallel": "2.0.1",
    "pino-pretty": "4.1.0",
    "rimraf": "3.0.2",
    "ts-jest": "26.2.0",
    "tsc-watch": "4.2.9",
    "tslint": "6.1.3",
    "tslint-config-airbnb": "5.11.2",
    "tslint-eslint-rules": "5.4.0",
    "tslint-immutable": "6.0.1",
    "typescript": "3.9.7"
  }
}
